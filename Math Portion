 #include <iostream>
  2 #include <vector>
  3 #include "calc.h"
  4 #include <cmath>
  5 using namespace std;
  6
  7 vector<string> input;
  8 vector<calc*> table(26,new calc);
  9
 10
 11
 12
 13 unsigned char math(vector<string> input) {
 14 int counter = 1;
 15 unsigned char value= get_val(input.at(0));
 16
 17
 18 while(counter < (int) input.size()) {
 19 char operand = input.at(counter).at(0);
 20 counter++;
 21
 22 unsigned char second_var = get_val(input.at(counter));
 23
 24 if (operand == '+') {
 25     value = value + second_var;
 26 }
 27 if (operand == '-') {
 28     value = value - second_var;
 29 }
 30 if (operand == '*') {
 31     value = value * second_var;
 32 }
 33 if (operand == '/') {
 34     value = value / second_var;
 35 }
 36 if (operand == '%') {
 37     value = value % second_var;
 38 }
 39 if (operand == '^') {
 40     value = value^second_var;
 41         }
 42     }
 43 return value;
 44 }
